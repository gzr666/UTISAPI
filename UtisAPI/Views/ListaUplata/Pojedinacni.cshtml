@model UtisAPI.Models.YearlyMembership

<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/css")
    @Scripts.Render("~/bundles/modernizr")

    </head>

<body>



    <h4>Uplatnica: @Model.User.Name @Model.User.Surname</h4>
	<hr />

    <fieldset>

            <span><strong>ID: </strong></span><span>@Model.ID</span><br/>
             <span><strong>Vrsta Uplate: </strong></span><span>@Model.VrstaUplate.Name</span><br />
            <span><strong>Za godinu: </strong></span><span>@Model.Year.BeginDate.Year</span><br />
            <span><strong>Datum uplate: </strong></span><span>@Model.DatumUplate.ToString("d/M/yyyy")</span><br />
            <span><strong>Iznos: </strong></span><span>@Model.Amount kn</span><br /><br/>
        <hr/>
        <span><strong>Potpis: </strong></span><span class="potpis"></span><br />





        </fieldset>
   


    </body>



</html>